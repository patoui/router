{
    "name": "patoui/router",
    "description": "",
    "keywords": [
        "patoui",
        "router"
    ],
    "homepage": "https://github.com/patoui/router",
    "license": "MIT",
    "authors": [
        {
            "name": "Patrique Ouimet",
            "email": "patriqueouimet@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "8.x",
        "ext-json": "*",
        "psr/http-factory": "^1.0",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "phpstan/phpstan": "^0.12.3",
        "phpunit/phpunit": "^9.5",
        "phpspec/prophecy": "^1.9",
        "symfony/var-dumper": "^4.3",
        "vimeo/psalm": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Patoui\\Router\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Patoui\\Router\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    }
}
